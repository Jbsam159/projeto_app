{
  "expo": {
    "name": "projeto",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permiss√£o para acessar as fotos"
        }
      ]
    ],
    "slug": "projeto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
